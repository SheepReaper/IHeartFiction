{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>SheepReaper/renovate-config"
  ],
  "enabledManagers": [
    "github-actions",
    "nuget",
    "renovate-config-presets",
    "custom.regex"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "Directory.Packages.props"
      ],
      "matchStrings": [
        "<PackageVersion\\s+Include=\"(?<depName>[^\"]+)\"\\s+Version=\"\\[(?<currentValue>[^,]+),.*\""
      ],
      "datasourceTemplate": "nuget",
      "versioningTemplate": "nuget"
    }
  ]
}