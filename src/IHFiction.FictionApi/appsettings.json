{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "localhost.localdomain,localhost",
  "KeycloakAdminClientOptions": {
    "AuthClientSecret": "ADMIN_CLIENT_SECRET"
  },
  "Markdown": {
    "MaxBase64ImageSizeBytes": 5242880,
    "AllowInsecureHttp": false,
    "AllowAllHttpsLinks": true,
    "AllowRelativeLinks": false
  },
  "Cors":{
    "AllowedOrigins": "https://localhost.localdomain,https://localhost"
  }
}
